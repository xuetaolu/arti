/* pages/notice/notice.wxss */
.page-head{
  height: 14.3%;
  display: flex;
  flex-direction: row;
  align-items: center;
  background-color: white;
}

.page-head-title{
  width: 28.7%;
  text-align: right;
  font-weight: 900;
}

.page-head-container{
  background-color: #eaeaec;
  margin: 0 8% 0 9.7%;
  width: 53.6%;
  height: 45.4%;
  display: flex;
  flex-direction: row;
  align-items: center;
  border-radius: 20px;
}

.page-head-container input{
  padding: 0 10rpx 0 50rpx;
}

.page-head-container-line{
  height: 100%;
  width: 5px;
  background-color: #f0f0f0;
}

.page-head-container-icon{
  height: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
  padding: 0 50rpx;
  border-top-right-radius:2em;
  border-bottom-right-radius:2em;
}

.page-head-container-icon:active{
  background-color: #f6f8f9;
}

.page-head-container-icon image{
  height: 50rpx;
  width: 50rpx;
}

.page-content{
  height: 85.7%;
  

  background-color: #f1f2f3;
}

.page-content-item{

}

.page-content-item-title{
  background-color: #e5e6e9;
}

.page-content-item-date{

}